<Window x:Name="windowMain" x:Class="FirstWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FirstWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textName" HorizontalAlignment="Left" Margin="355,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged_1" RenderTransformOrigin="-0.358,0.615"/>
        <DatePicker x:Name="birthDate" HorizontalAlignment="Left" Margin="355,124,0,0" VerticalAlignment="Top" Height="26"/>
        <Label Content="Enter your Name:" HorizontalAlignment="Left" Margin="163,72,0,0" VerticalAlignment="Top"/>
        <Label Content="Enter your Birthday:" HorizontalAlignment="Left" Margin="163,119,0,0" VerticalAlignment="Top"/>
        <Button x:Name="submitButton" Content="Submit" HorizontalAlignment="Left" Margin="299,186,0,0" VerticalAlignment="Top" Click="Button_Click" Background="White"/>

    </Grid>
</Window>
